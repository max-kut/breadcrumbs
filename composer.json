{
    "name": "glissmedia/breadcrumbs",
    "description": "Package Breadcrumbs for Laravel ^5.5",
    "keywords": ["laravel", "breadcrumbs"],
    "license": "MIT",
    "homepage": "https://github.com/glissmedia/breadcrumbs",
    "authors": [
        {
            "name": "GlissmediaTeam",
            "email": "support@glissmedia.ru",
            "homepage": "http://glissmedia.ru"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "^5.5"
    },
    "autoload": {
        "classmap": [],
        "files": [],
        "psr-4": {
            "Glissmedia\\Breadcrumbs\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Glissmedia\\Breadcrumbs\\BreadcrumbsServiceProvider"
            ],
            "aliases": {
                "Breadcrumbs": "Glissmedia\\Breadcrumbs\\BreadcrumbsFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
